{
	"name": "smntcs-custom-logo-link",
	"description": "Allows to customize the logo link.",
	"author": "Niels Lange",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Custom Logo Link"
	],
	"homepage": "https://github.com/nielslange/smntcs-custom-logo-link#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nielslange/smntcs-custom-logo-link.git"
	},
	"bugs": {
		"url": "https://github.com/nielslange/smntcs-custom-logo-link/issues"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@wordpress/env": "4.1.3",
		"@wordpress/prettier-config": "1.1.1",
		"cypress": "^9.2",
		"eslint": "^8.6.0",
		"npm-run-all": "4.1.5",
		"prettier": "npm:wp-prettier@2.0.5"
	},
	"scripts": {
		"cypress": "cypress",
		"phpcbf": "composer phpcbf",
		"phpcs": "composer phpcs",
		"phplint": "composer phplint",
		"test": "npm-run-all --parallel phpcs phplint",
		"test:e2e": "cypress run --headless --browser chrome",
		"test:e2e-dev": "cypress open",
		"test:theme-astra": "cypress run --headless --browser chrome --spec 'cypress/integration/astra.js'",
		"test:theme-atomic-blocks": "cypress run --headless --browser chrome --spec 'cypress/integration/atomic-blocks.js'",
		"test:theme-blocksy": "cypress run --headless --browser chrome --spec 'cypress/integration/blocksy.js'",
		"test:theme-colormag": "cypress run --headless --browser chrome --spec 'cypress/integration/colormag.js'",
		"test:theme-customify": "cypress run --headless --browser chrome --spec 'cypress/integration/customify.js'",
		"test:theme-cuisine": "cypress run --headless --browser chrome --spec 'cypress/integration/cuisine.js'",
		"test:theme-education-hub": "cypress run --headless --browser chrome --spec 'cypress/integration/education-hub.js'",
		"test:theme-generatepress": "cypress run --headless --browser chrome --spec 'cypress/integration/generatepress.js'",
		"test:theme-hello-elementor": "cypress run --headless --browser chrome --spec 'cypress/integration/hello-elementor.js'",
		"test:theme-hestia": "cypress run --headless --browser chrome --spec 'cypress/integration/hestia.js'",
		"test:theme-lore": "cypress run --headless --browser chrome --spec 'cypress/integration/lore.js'",
		"test:theme-neve": "cypress run --headless --browser chrome --spec 'cypress/integration/neve.js'",
		"test:theme-nevertheless": "cypress run --headless --browser chrome --spec 'cypress/integration/nevertheless.js'",
		"test:theme-oceanwp": "cypress run --headless --browser chrome --spec 'cypress/integration/oceanwp.js'",
		"test:theme-osmosis": "cypress run --headless --browser chrome --spec 'cypress/integration/osmosis.js'",
		"test:theme-shapely": "cypress run --headless --browser chrome --spec 'cypress/integration/shapely.js'",
		"test:theme-storefront": "cypress run --headless --browser chrome --spec 'cypress/integration/storefront.js'",
		"test:theme-suffice": "cypress run --headless --browser chrome --spec 'cypress/integration/suffice.js'",
		"test:theme-sydney": "cypress run --headless --browser chrome --spec 'cypress/integration/sydney.js'",
		"test:theme-twentyfifteen": "cypress run --headless --browser chrome --spec 'cypress/integration/twentyfifteen.js'",
		"test:theme-twentynineteen": "cypress run --headless --browser chrome --spec 'cypress/integration/twentynineteen.js'",
		"test:theme-twentyseventeen": "cypress run --headless --browser chrome --spec 'cypress/integration/twentyseventeen.js'",
		"test:theme-twentysixteen": "cypress run --headless --browser chrome --spec 'cypress/integration/twentysixteen.js'",
		"test:theme-twentytwenty": "cypress run --headless --browser chrome --spec 'cypress/integration/twentytwenty.js'",
		"test:theme-xi-portfolio": "cypress run --headless --browser chrome --spec 'cypress/integration/xi-portfolio.js'",
		"wp-env": "wp-env",
		"wp-env:rc": "wp-env run cli 'wp core update --version=6.2-RC1 && wp core update --version=6.2-RC1 --force'"
	}
}
