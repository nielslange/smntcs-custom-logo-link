{
	"name": "smntcs-custom-logo-link",
	"description": "Allows to customize the logo link.",
	"author": "Niels Lange",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Custom Logo Link"
	],
	"homepage": "https://github.com/nielslange/smntcs-custom-logo-link#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nielslange/smntcs-custom-logo-link.git"
	},
	"bugs": {
		"url": "https://github.com/nielslange/smntcs-custom-logo-link/issues"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@wordpress/env": "10.9.0",
		"@wordpress/prettier-config": "4.9.0",
		"cypress": "^13.15.0",
		"eslint": "^9.12.0",
		"npm-run-all": "4.1.5",
		"prettier": "npm:wp-prettier@3.0.3"
	},
	"scripts": {
		"cypress": "cypress",
		"phpcbf": "composer phpcbf",
		"phpcs": "composer phpcs",
		"phplint": "composer phplint",
		"test": "npm-run-all --parallel phpcs phplint",
		"test:e2e": "cypress run --headless --browser chrome",
		"test:e2e-dev": "cypress open --config-file=cypress.config.js",
		"test:theme-astra": "cypress run --headless --browser chrome --spec 'cypress/e2e/astra.cy.js'",
		"test:theme-atomic-blocks": "cypress run --headless --browser chrome --spec 'cypress/e2e/atomic-blocks.cy.js'",
		"test:theme-blocksy": "cypress run --headless --browser chrome --spec 'cypress/e2e/blocksy.cy.js'",
		"test:theme-colormag": "cypress run --headless --browser chrome --spec 'cypress/e2e/colormag.cy.js'",
		"test:theme-customify": "cypress run --headless --browser chrome --spec 'cypress/e2e/customify.cy.js'",
		"test:theme-cuisine": "cypress run --headless --browser chrome --spec 'cypress/e2e/cuisine.cy.js'",
		"test:theme-education-hub": "cypress run --headless --browser chrome --spec 'cypress/e2e/education-hub.cy.js'",
		"test:theme-generatepress": "cypress run --headless --browser chrome --spec 'cypress/e2e/generatepress.cy.js'",
		"test:theme-hello-elementor": "cypress run --headless --browser chrome --spec 'cypress/e2e/hello-elementor.cy.js'",
		"test:theme-hestia": "cypress run --headless --browser chrome --spec 'cypress/e2e/hestia.cy.js'",
		"test:theme-lore": "cypress run --headless --browser chrome --spec 'cypress/e2e/lore.cy.js'",
		"test:theme-neve": "cypress run --headless --browser chrome --spec 'cypress/e2e/neve.cy.js'",
		"test:theme-nevertheless": "cypress run --headless --browser chrome --spec 'cypress/e2e/nevertheless.cy.js'",
		"test:theme-oceanwp": "cypress run --headless --browser chrome --spec 'cypress/e2e/oceanwp.cy.js'",
		"test:theme-osmosis": "cypress run --headless --browser chrome --spec 'cypress/e2e/osmosis.cy.js'",
		"test:theme-shapely": "cypress run --headless --browser chrome --spec 'cypress/e2e/shapely.cy.js'",
		"test:theme-storefront": "cypress run --headless --browser chrome --spec 'cypress/e2e/storefront.cy.js'",
		"test:theme-suffice": "cypress run --headless --browser chrome --spec 'cypress/e2e/suffice.cy.js'",
		"test:theme-sydney": "cypress run --headless --browser chrome --spec 'cypress/e2e/sydney.cy.js'",
		"test:theme-twentyfifteen": "cypress run --headless --browser chrome --spec 'cypress/e2e/twentyfifteen.cy.js'",
		"test:theme-twentynineteen": "cypress run --headless --browser chrome --spec 'cypress/e2e/twentynineteen.cy.js'",
		"test:theme-twentyseventeen": "cypress run --headless --browser chrome --spec 'cypress/e2e/twentyseventeen.cy.js'",
		"test:theme-twentysixteen": "cypress run --headless --browser chrome --spec 'cypress/e2e/twentysixteen.cy.js'",
		"test:theme-twentytwenty": "cypress run --headless --browser chrome --spec 'cypress/e2e/twentytwenty.cy.js'",
		"test:theme-xi-portfolio": "cypress run --headless --browser chrome --spec 'cypress/e2e/xi-portfolio.cy.js'",
		"wp-env": "wp-env",
		"wp-env:rc": "wp-env run cli 'wp core update --version=6.2-RC1 && wp core update --version=6.2-RC1 --force'"
	}
}
