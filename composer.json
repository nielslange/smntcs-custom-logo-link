{
	"name": "nielslange/smntcs-custom-logo-link",
	"type": "plugin",
	"description": "Allows to customize the logo link.",
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Custom Logo Link"
	],
	"homepage": "https://github.com/nielslange/smntcs-custom-logo-link",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Contributors",
			"homepage": "https://github.com/nielslange/smntcs-custom-logo-link/graphs/contributors"
		}
	],
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"wptrt/wpthemereview": "^0.2.1",
		"humanmade/psalm-plugin-wordpress": "^2.0"
	},
	"scripts": {
		"checkcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs --runtime-set testVersion 5.6-",
		"fixcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf --runtime-set testVersion 5.6-",
		"lint": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
	},
	"support": {
		"issues": "https://github.com/nielslange/smntcs-custom-logo-link/issues",
		"source": "https://github.com/nielslange/smntcs-custom-logo-link"
	}
}
